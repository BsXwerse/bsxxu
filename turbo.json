{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["//#lint", "//#service:check", "^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["//#service:check"]
    },
    "//#lint": {},
    "//#service:up": {},
    "//#service:check": {
      "dependsOn": ["//#service:up"]
    },
    "@bsxxu/web#build": {
      "dependsOn": ["@bsxxu/server#build"]
    }
  }
}
